clear all;

number=0.636;

printf("Number: %f\n", number);
printf("cos(%f)\t%f\n", number, cos(number*%pi));
printf("sin(%f)\t%f\n", number, sin(number*%pi));
printf("acos(%f)\t%f\n", number, acos(number)/%pi);
printf("asin(%f)\t%f\n", number, asin(number)/%pi);
printf("rand()\t%f\n", rand());
printf("sqrt(%f)\t%f\n", number, sqrt(number));
printf("abs(%f)\t%f\n", number, abs(number));
printf("ln(%f)\t%f\n", number, log(number));
printf("log2(%f)\t%f\n", number, log(number)/log(2));
printf("log10(%f)\t%f\n", number, log(number)/log(10));
printf("exp(%f)\t%f\n", number, exp(number));
printf("pow(0.5, %f)\t%f\n", number, 0.5^number);
